#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/cli.js/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/envinfo@7.8.1/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules:/home/amritesh/Workspace/React/node_modules:/home/amritesh/Workspace/node_modules:/home/amritesh/node_modules:/home/node_modules:/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/node_modules/envinfo/dist/cli.js/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/node_modules/envinfo/dist/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/node_modules/envinfo/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../envinfo/dist/cli.js" "$@"
else 
  exec node  "$basedir/../envinfo/dist/cli.js" "$@"
fi
