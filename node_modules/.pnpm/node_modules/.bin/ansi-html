#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/bin/ansi-html/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/bin/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/ansi-html@0.0.7/node_modules/ansi-html/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/ansi-html@0.0.7/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules:/home/amritesh/Workspace/React/node_modules:/home/amritesh/Workspace/node_modules:/home/amritesh/node_modules:/home/node_modules:/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/node_modules/ansi-html/bin/ansi-html/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/node_modules/ansi-html/bin/node_modules:/home/amritesh/Workspace/React/billing-app/node_modules/.pnpm/node_modules/ansi-html/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ansi-html/bin/ansi-html" "$@"
else 
  exec node  "$basedir/../ansi-html/bin/ansi-html" "$@"
fi
